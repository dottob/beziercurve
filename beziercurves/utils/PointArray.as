package beziercurves.utils {	import beziercurves.display.AnchorPoint;	import beziercurves.display.ControlPoint;	//import flash.geom.Point;	//import flash.utils.getQualifiedSuperclassName;		/**	 * Copyright (c) 2008 nicolas levavasseur	 * nicolas.levavasseur@gmail.com	 * 	 * @author nicolas levavasseur	 */	public dynamic class PointArray extends Array {		/**		 * create the array of points		 */		public function PointArray(){			super();		}				/**		 * export the array in the output		 * the result is		 * 		 * //------- CURVE DEFINITION -------		 * var b:BezierCurve = new BezierCurve();		 * ...		 * b.addAnchor(x, y);		 * ...		 * b.addControl(x, y);		 * ...		 * b.addAnchor(x, y);		 * ...		 * //--------------------------------		 */		public function export():void {			trace("//------- CURVE DEFINITION -------");			trace("var b:BezierCurve = new BezierCurve();");			for(var i:int = 0; i < length; i++){				switch( this[i].constructor ){					case AnchorPoint:						trace("b.addAnchor(" + this[i].x + ", " + this[i].y + ");");						break;					case ControlPoint:						trace("b.addControl(" + this[i].x + ", " + this[i].y + ");");						break;				}			}			trace("addChild(b);");			trace("//--------------------------------");		}				/**		 * copy the content of a normal Array into a new PointArray		 */		public static function copy(array:Array):PointArray {			var a:PointArray = new PointArray();			for(var i:int = 0; i < array.length; i++){				a.push(array[i]);			}			return a;		}				/**		 * add one or more AnchorPoint, ControlPoint at the end of the PointArray		 */		AS3 override function push(... args):uint {			for each(var arg:Object in args){				//if( arg.constructor !== Point && getQualifiedSuperclassName(arg) !== "flash.geom::Point" ) {				if( arg.constructor !== ControlPoint && arg.constructor !== AnchorPoint ) {					throw new Error("one of the arguments is not valid, check types\n\tat beziercurves.utils::PointArray.push()");					return this.length;				}			}			return super.push.apply(null, args);		}	}}